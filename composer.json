{
    "name": "filestack/filestack-php",
    "description": "A Filestack library for PHP",
    "license": "Apache-2.0",
    "require": {
        "php" : ">=7.3",
        "guzzlehttp/guzzle": "~6.5 || ~7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "dev-master",
        "phpmd/phpmd" : "@stable",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4" : {
            "Filestack\\" : "filestack",
            "Filestack\\Mixins\\" : "filestack/mixins"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Filestack\\Tests\\": "tests"
        }
    }
}
